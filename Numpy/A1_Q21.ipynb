     "text": [
      "Original Array: [7 8 3 2 4]\n",
      "Reversed Array: [4 2 3 8 7]\n"
     ]
    }
   ],
   "source": [
    "arr = np.random.randint(0,9,5)\n",
    "print(\"Original Array:\",arr)\n",
    "\n",
    "arr =arr[::-1]\n",
    "print(\"Reversed Array:\",arr)"
